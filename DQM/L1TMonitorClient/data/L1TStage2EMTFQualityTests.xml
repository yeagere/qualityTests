<TESTSCONFIGURATION>

<!-- Tests for *emtfChamber hot  -->

    <QTEST name="NoisyChannel_Chamber_Hot">
	<TYPE>NoisyChannel</TYPE>	
	<PARAM name="tolerance">2</PARAM>
	<PARAM name="neighboursX">36</PARAM>
	<PARAM name="neighboursY">120</PARAM>
	<PARAM name="error">0.50</PARAM>
	<PARAM name="warning">0.70</PARAM>
    </QTEST>

    <LINK name="*emtfChamberStrip_QT_hot*">
        <TestName activate="true">NoisyChannel_Chamber_Hot</TestName>
    </LINK>

<!-- Tests for *emtfChamber dead  -->

